{
  "name": "Remover Nonio",
  "version": "2.0.7",
  "description": "Remove o pedido de registo na Nonio",
  "permissions": [
    "activeTab",
    "tabs",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "browser_action": {
    "default_icon": "images/get_started32.png",
    "default_title": "Removedor da Nonio"
  },
  "manifest_version": 2
}
