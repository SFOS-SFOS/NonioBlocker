{
  "name": "MyAdBlocker",
  "version": "2.1",
  "description": "Remove publicidade irritante",
  "permissions": [
    "activeTab",
    "tabs",
    "webRequest",
    "<all_urls>",
    "webRequestBlocking",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "src/images/get_started16.png",
    "32": "src/images/get_started32.png",
    "48": "src/images/get_started48.png",
    "128": "src/images/get_started128.png"
  },
  "browser_action": {
    "default_icon": "src/images/get_started32.png",
    "default_title": "Bloqueador de publicidade"
  },
  "manifest_version": 2
}
