{
  "name": "MyAdBlocker",
  "version": "2.0.12",
  "description": "Remove o pedido de registo na Nonio e outros elementos irritantes",
  "permissions": [
    "activeTab",
    "tabs",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "src/images/get_started16.png",
    "32": "src/images/get_started32.png",
    "48": "src/images/get_started48.png",
    "128": "src/images/get_started128.png"
  },
  "browser_action": {
    "default_icon": "src/images/get_started32.png",
    "default_title": "Bloqueador da Nonio e publicidade"
  },
  "manifest_version": 2
}
